@model List<mess.EntFrame.My_Table>
<link href="~/MyTemp/css/style.css" rel="stylesheet" type="text/css" />
<div style="width:90%; margin:0 auto;">
<table id="hello">
<thead>
<tr>
<th>ID Number</th>
<th>First Name</th>
<th>Last Name</th>
<th>UserName</th>
<th>Password</th>
<th>What Do You Want?</th>
<td>
@foreach (var a in Model)
 { 
      <tr>
      <td><center> @a.ID</center></td>
      <td><center> @a.fname</center></td>
      <td><center>@a.lname</center></td>
      <td><center>@a.Uname</center></td>
      <td><center>@a.Pword</center></td>
      <td>
        <a href ="/Create/Edit/@a.ID">Edit</a>
        <a href ="/Create/Delete/@a.ID">Delete</a>
     </td>
     </tr>
 }
 </td>
 </tr>
</thead>
</table>
