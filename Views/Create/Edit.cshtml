@model mess.EntFrame.My_Table

@using (Html.BeginForm())
{

<div class = "container">
<div class ="registrationform">
<div class="container" style="width:40%; margin-top:2%">

<center><h5>@Html.TextBoxFor(s => s.ID, new { @class = "form-control", @placeholder = "ID Number" })
@Html.ValidationMessageFor(s => s.ID, "", new { @class = "error" })
</h5></center>

<center><h5> @Html.TextBoxFor(s => s.fname, new { @class = "form-control", @placeholder = "First Name" })
@Html.ValidationMessageFor(s => s.fname, "", new { @class = "error" })
</h5></center>
<center><h5> @Html.TextBoxFor(s => s.lname, new { @class = "form-control", @placeholder = "Last Name" })
@Html.ValidationMessageFor(s => s.lname, "", new { @class = "error" })
</h5></center>

<input type="submit" value="Edit" class="btn btn-block btn-primary"/>
</div>
</div>
</div>

}
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
